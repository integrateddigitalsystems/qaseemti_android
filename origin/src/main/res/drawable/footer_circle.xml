<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item>
    <shape android:shape="oval"
        android:useLevel="false" >

        <size
            android:height="50dp"
            android:width="60dp" />
        <gradient android:angle="-90" android:startColor="@color/start_color_gray"
            android:endColor="@color/end_color_gray"  />

    </shape>

</item>
<item
    android:bottom="5dp"
    android:left="6dp"
    android:right="6dp"
    android:top="5dp">
    <shape android:shape="oval" >
        <solid android:color="@color/white" />

    </shape>
</item>

</layer-list>